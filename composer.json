{
    "name": "nunomaduro/collision-adapter-symfony",
    "description": "Collision's adapter for Symfony applications. Error Reporting for console/command-line PHP applications.",
    "keywords": ["symfony", "bundle", "console", "command-line", "php", "cli", "error", "handling", "collision", "laravel-zero"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "nunomaduro/collision": "^3.0",
        "symfony/framework-bundle": "^4.0|^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "^7.5",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/yaml": "^4.0|^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NunoMaduro\\CollisionAdapterSymfony\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
